@model List<CC9.Models.Customer>

@{
    ViewBag.Title = "CustomersInGermany";
}

<h2>CustomersInGermany</h2>



<table border="1">
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>
        <th>Country</th>
        <th>City</th>
    </tr>

    @foreach (var c in Model)
    {
        <tr>
            <td>@c.CustomerID</td>
            <td>@c.CompanyName</td>
            <td>@c.ContactName</td>
            <td>@c.Country</td>
            <td>@c.City</td>
        </tr>
    }
</table>


